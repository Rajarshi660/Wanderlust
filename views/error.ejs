<% layout("/layout/boilerplate") %>
<div class="row mt-3">
    <div class="col-8 offset-2">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><i class="fa-solid fa-circle-exclamation"></i> Error!</h4>
            <p><%= err.message %></p>
            <hr>
            <p class="mb-0">Status Code: <%= err.statusCode %></p>
        </div>
        <a href="/listings" class="btn btn-outline-dark mt-3">Back to Listings</a>
    </div>
</div>